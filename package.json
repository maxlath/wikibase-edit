{
  "name": "wikidata-edit",
  "version": "1.6.0",
  "description": "Edit Wikidata from NodeJS",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "mocha test/* test/*/*",
    "prepublish": "npm run lint && npm run test",
    "update-properties": "./scripts/update_properties",
    "update-toc": "doctoc README.md docs/*.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxlath/wikidata-edit.git"
  },
  "keywords": [
    "wikidata",
    "edit",
    "API"
  ],
  "author": "maxlath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxlath/wikidata-edit/issues"
  },
  "homepage": "https://github.com/maxlath/wikidata-edit#readme",
  "dependencies": {
    "bluereq": "^2.1.2",
    "chalk": "^1.1.3",
    "wikidata-sdk": "^5.1.4",
    "wikidata-token": "^2.3.2"
  },
  "devDependencies": {
    "config": "^1.25.1",
    "doctoc": "^1.3.0",
    "mocha": "^3.2.0",
    "should": "^11.2.1",
    "standard": "^9.0.2",
    "wikidata-cli": "^4.8.1"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "globals": [
      "it",
      "describe"
    ]
  },
  "engines": {
    "node": ">= 6.4"
  }
}
